{
  "displayName": "Elysia Frontend",
  "name": "elysia_frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": {
    "name": "Alex Navarro",
    "email": "alex@navarrotech.net"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "yarn && vite --host",
    "build": "npx rimraf dist && tsc -b && vite build",
    "preview": "vite preview",
    "analyze": "vite-bundle-visualizer",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --dom --coverage",
    "test:dev": "cross-env DEBUG_PRINT_LIMIT=100000 && vitest watch --ui --dom --coverage.enabled=false --reporter verbose",
    "lint": "eslint . --ext ts,tsx --max-warnings 0",
    "i18n": "tsx scripts/ensure_consistent_translations.ts",
    "update-common": "tsx scripts/update_from_common.ts",
    "ensure-translation": "tsx scripts/ensure_consistent_translations.ts",
    "purge": "yarn clean && npx rimraf node_modules/",
    "clean": "npx rimraf test/ && npx rimraf dist/",
    "reinstall": "yarn purge && yarn install",
    "preflight": "yarn && yarn test && yarn lint && yarn typecheck && yarn i18n && yarn build && yarn clean && echo Preflight complete, all set to send!"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@navarrotech/bulma": "^1.1.1",
    "@reduxjs/toolkit": "^2.5.0",
    "bulma": "^1.0.2",
    "bulma-smart-react-forms": "^0.0.8-alpha",
    "events": "^3.3.0",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "ky": "^1.7.3",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.0",
    "react-password-strength-bar": "^0.4.1",
    "react-redux": "^9.2.0",
    "react-router": "^6",
    "react-router-dom": "^6",
    "redux-persist": "^6.0.0",
    "sass": "^1.83.0",
    "uuid": "^11.0.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin-js": "^2.12.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.12.0",
    "happy-dom": "^15.11.7",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.2.0",
    "vite-plugin-full-reload": "^1.2.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.4"
  }
}
